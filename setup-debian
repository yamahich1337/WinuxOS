
echo ""
echo "Welcome to final of setup"
sleep 3
clear
apt update
clear
apt upgrade
clear
echo ""
echo "installing plasma by kde"
apt install kde-plasma-desktop
clear
echo ""
echo "installing window manager"
apt install openbox-kde-session
clear
apt install wine
clear
apt install firefox-esr
clear
export DISTRIB_DESCRIPTION="WinuxOS"
clear
source ~/.bashrc
clear

echo ""
echo "Thank you for installing our OS. Run next-time by ./winux"


