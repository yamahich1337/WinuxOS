cd
 chmod +x ./setup-debian
echo ""
echo "Welcome to la finale of setup. Please be patient."
apt update>/dev/null 2>&1
apt upgrade>/dev/null 2>&1
apt install kde-plasma-desktop>/dev/null 2>&1
apt install wine>/dev/null 2>&1
apt install firefox>/dev/null 2>&1
export DISTRIB_DESCRIPTION="WinuxOS">/dev/null 2>&1
source ~/.bashrc>/dev/null 2>&1

echo ""
echo "Thank you for installing our OS. Run next-time by ./winux"


